FROM alpine:3.22.2

# 设置阿里云镜像仓库
RUN sed -i 's/dl-cdn.alpinelinux.org/mirrors.aliyun.com/g' /etc/apk/repositories
ENV TimeZone=Asia/Shanghai
RUN ln -sf /usr/share/zoneinfo/$TimeZone /etc/localtime && echo $TimeZone > /etc/timezone
# 安装必要的软件包以及依赖 字体
RUN apk update && apk add --no-cache fontconfig ttf-dejavu
# 下载特殊字体
RUN apk add --no-cache curl tar
# 安装jdk
RUN curl -L http://172.16.0.227:84/jdk/Linux/OpenJDK25U-jdk_x64_alpine-linux_hotspot_25.0.1_8.tar.gz -o /tmp/jdk.tar.gz && \
    mkdir -p /opt/java && \
    tar -xzf /tmp/jdk.tar.gz -C /opt/java --strip-components=1 && \
    rm /tmp/jdk.tar.gz

ENV JAVA_HOME=/opt/java
ENV PATH=$JAVA_HOME/bin:$PATH

