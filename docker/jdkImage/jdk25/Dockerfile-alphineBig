     curl -o "/usr/share/fonts/trebucbi.ttf" http://172.16.0.227:84/fonts/trebucbi.ttf && \
     curl -o "/usr/share/fonts/trebucit.ttf" http://172.16.0.227:84/fonts/trebucit.ttf && \
     curl -o "/usr/share/fonts/verdana.ttf" http://172.16.0.227:84/fonts/verdana.ttf && \
     curl -o "/usr/share/fonts/verdanab.ttf" http://172.16.0.227:84/fonts/verdanab.ttf && \
     curl -o "/usr/share/fonts/verdanai.ttf" http://172.16.0.227:84/fonts/verdanai.ttf && \
     curl -o "/usr/share/fonts/verdanaz.ttf" http://172.16.0.227:84/fonts/verdanaz.ttf && \
     curl -o "/usr/share/fonts/webdings.ttf" http://172.16.0.227:84/fonts/webdings.ttf && \
     curl -o "/usr/share/fonts/wingding.ttf" http://172.16.0.227:84/fonts/wingding.ttf && \
     curl -o "/usr/share/fonts/wingdings 2.ttf" http://172.16.0.227:84/fonts/wingdings%202.ttf

RUN apk del curl tar
# 删除缓存
RUN rm -rf /var/cache/apk/* && rm -rf /tmp/*